{
    "name": "webhook-notifier",
    "version": "0.1.0",
    "description": "GitHub Action to send messages to different webhooks",
    "keywords": [
        "actions",
        "webhook",
        "notifier",
        "notification",
        "node",
        "discord",
        "teams",
        "slack"
    ],
    "homepage": "https://github.com/Netail/webhook-notifier#readme",
    "license": "MIT",
    "author": "Netail",
    "main": "lib/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Netail/webhook-notifier.git"
    },
    "scripts": {
        "format": "prettier --write src/**/*.ts",
        "format:check": "prettier --check src/**/*.ts",
        "lint": "eslint src/**/*.ts",
        "build": "tsc",
        "build:dist": "yarn build && ncc build -o dist --source-map",
        "precommit": "yarn format && yarn lint && yarn build:dist"
    },
    "dependencies": {
        "@actions/core": "^1.10.0",
        "axios": "^1.2.1"
    },
    "devDependencies": {
        "@types/node": "^18.11.10",
        "@typescript-eslint/parser": "^5.45.0",
        "@vercel/ncc": "^0.34.0",
        "eslint": "^8.28.0",
        "eslint-plugin-github": "^4.4.1",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "latest",
        "typescript": "^4.9.3"
    },
    "private": false
}
