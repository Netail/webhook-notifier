{
    "name": "webhook-notifier",
    "version": "0.1.0",
    "description": "GitHub Action to send messages to different webhooks",
    "keywords": [
        "actions",
        "webhook",
        "notifier",
        "notification",
        "node",
        "discord",
        "teams",
        "slack"
    ],
    "homepage": "https://github.com/Netail/webhook-notifier#readme",
    "license": "MIT",
    "author": "Netail",
    "main": "lib/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Netail/webhook-notifier.git"
    },
    "scripts": {
        "build": "tsc",
        "format": "prettier --write src/**/*.ts",
        "format:check": "prettier --check src/**/*.ts",
        "lint": "eslint src/**/*.ts",
        "bundle": "ncc build -o dist --source-map",
        "precommit": "yarn lint && yarn format && yarn build && yarn bundle"
    },
    "dependencies": {
        "@actions/core": "^1.4.0",
        "axios": "^0.21.1"
    },
    "devDependencies": {
        "@types/node": "^15.12.4",
        "@typescript-eslint/parser": "^4.28.4",
        "@vercel/ncc": "^0.28.6",
        "eslint": "^7.31.0",
        "eslint-plugin-github": "^4.1.5",
        "eslint-plugin-prettier": "^3.4.0",
        "prettier": "^2.3.1",
        "typescript": "^4.3.5"
    },
    "private": false
}
